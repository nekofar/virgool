{
  "name": "nekofar/virgool",
  "type": "wordpress-plugin",
  "description": "Virgool lets you publish posts automatically to a Virgool profile.",
  "homepage": "https://github.com/nekofar/virgool",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Milad Nekofar",
      "email": "milad@nekofar.com"
    }
  ],
  "require": {
    "php": ">=7.0 || >=8.0",
    "ext-json": "*",
    "composer/installers": "v2.1.1"
  },
  "require-dev": {
    "codeception/codeception": "4.2.1",
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
    "friendsofphp/php-cs-fixer": "v3.9.5",
    "lucatume/wp-browser": "3.1.6",
    "phpcompatibility/phpcompatibility-wp": "2.1.3",
    "phpunit/phpunit": "9.5.22",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "3.7.1",
    "wordpress/wordpress": "5.8",
    "wp-cli/dist-archive-command": "v2.0.1",
    "wp-cli/wp-cli": "2.6.0",
    "wp-coding-standards/wpcs": "2.3.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "wordpress-core",
        "version": "5.8",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/5.8.zip"
        }
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": "./vendor/bin/phpcs"
  }
}
